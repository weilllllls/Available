<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>RequestInfoServlet</servlet-name>
        <servlet-class>com.available.servlet.RequestInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RequestInfoServlet</servlet-name>
        <url-pattern>/RequestInfo</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>SearchRequestServlet</servlet-name>
        <servlet-class>com.available.servlet.SearchRequestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchRequestServlet</servlet-name>
        <url-pattern>/SearchRequest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SubmitInfoServlet</servlet-name>
        <servlet-class>com.available.servlet.SubmitInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SubmitInfoServlet</servlet-name>
        <url-pattern>/SubmitInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletPC</servlet-name>
        <servlet-class>com.available.servlet.ServletPC</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletPC</servlet-name>
        <url-pattern>/ServletPC</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadInfoServlet</servlet-name>
        <servlet-class>com.available.servlet.UploadInfoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadInfoServlet</servlet-name>
        <url-pattern>/UploadInfo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletReply</servlet-name>
        <servlet-class>com.available.servlet.ServletReply</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletReply</servlet-name>
        <url-pattern>/ServletReply</url-pattern>
    </servlet-mapping>
</web-app>